
  public class Calculator{
    public static void main(String[] args){
			System.out.println("Hello to the calculator");
						int a = 1;		
						int b = 2;
		    	    
		    	       int y =(a + b);
		    	       int x =(b/a);
		    	       int v=(b-a);
		    	       int n=(a*b);
		    	    
		    	  
		    	      System.out.println(y);
		    	      System.out.println(x);
		    	      System.out.println(v);
		    	      System.out.println(n);
    }
  }
